@model IEnumerable<TeleCord.Models.MessagesElements>

@{
    ViewBag.Title = "SearchbyKeyword";
}

<h2>SearchbyKeyword</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Transmitter)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Reciever)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.text)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Transmitter)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Reciever)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.text)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
